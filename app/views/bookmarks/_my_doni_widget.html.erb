<div class='widget myDoni'>

<% if my.length > 0 %>
    <div class='widgetHeader'>
            <h3 class='widgetHeaderTitle'>I miei notebook</h3>
        <div class='widgetHeaderTools'>
            <p class='collapse'><a class='expanded' href='#' title='Collapse'>Collapse</a></p>
    	</div>
    </div>
    <div class='widgetContent expanded'>
        <% if my.length > 0 %>
            <%= render :partial => '/bookmarks/notebook_and_bookmarks.html', :object => my %>
        <% end %>
    </div>
<% end %>

<% if subscribed.length > 0 %>
    <div class='widgetHeader'>
            <h3 class='widgetHeaderTitle'>Altri notebook</h3>
        <div class='widgetHeaderTools'>
            <p class='collapse'><a class='expanded' href='#' title='Collapse'>Collapse</a></p>
    	</div>
    </div>
    <div class='widgetContent expanded'>
        <% if subscribed.length > 0 %>
            <%= render :partial => '/bookmarks/notebook_and_bookmarks.html', :object => subscribed %>
        <% end %>
    </div>
<% end %>

</div>