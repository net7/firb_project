        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://trac.talia.discovery-project.eu/wiki/HyperOntology#collocation -->

    <owl:DatatypeProperty rdf:about="#collocation">
